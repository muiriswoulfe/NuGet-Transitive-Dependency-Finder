<root>

  <xsd:schema id="root" xmlns="" xmlns:xsd="https://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="https://www.w3.org/XML/1998/namespace" />
    <xsd:element name="root">
      <xsd:complexType>
        <xsd:choice maxOccurs="unbounded">
          <xsd:element name="data">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="value" type="xsd:string" />
                <xsd:element name="comment" type="xsd:string" />
              </xsd:sequence>
              <xsd:attribute name="name" type="xsd:string" />
              <xsd:attribute ref="xml:space" />
            </xsd:complexType>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
    </xsd:element>
  </xsd:schema>

  <data name="ErrorMissingParameter" xml:space="preserve">
    <value>Error: The .NET solution file to analyze must be provided as a parameter.</value>
    <comment>The error message displayed when no command-line parameter is provided.</comment>
  </data>
  <data name="InfoCommencingAnalysis" xml:space="preserve">
    <value>Commencing analysis...</value>
    <comment>The message displayed when analysis is about to commence.</comment>
  </data>
  <data name="InfoNoTransitiveNuGetDependencies" xml:space="preserve">
    <value>No transitive NuGet dependencies found.</value>
    <comment>The message displayed when no transitive NuGet dependencies are found.</comment>
  </data>

</root>
