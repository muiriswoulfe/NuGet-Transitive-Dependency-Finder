<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
        <Configuration>
          <Format>cobertura,json,lcov,opencover,teamcity</Format>
          <Exclude>
            [NuGetTransitiveDependencyFinder.TestUtilities]*,
            [*]NuGetTransitiveDependencyFinder.ConsoleApp.Program,
            [*]NuGetTransitiveDependencyFinder.Wrappers.*
          </Exclude>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>
